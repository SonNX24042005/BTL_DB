-- Script import dữ liệu từ CSV files vào database qldt
-- Tạo bởi extract_csv_from_sql.py

USE qldt;

-- Tắt kiểm tra foreign key tạm thời
SET FOREIGN_KEY_CHECKS=0;

-- Import dữ liệu cho bảng bangdiem
LOAD DATA LOCAL INFILE 'bangdiem.csv'
INTO TABLE `bangdiem`
CHARACTER SET utf8mb4
FIELDS TERMINATED BY ','
ENCLOSED BY '"'
LINES TERMINATED BY '\n'
IGNORE 1 ROWS;

